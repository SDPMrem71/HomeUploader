{
  "name": "home-uploader",
  "version": "1.2.4",
  "description": "A file sharing server",
  "main": "HomeUploader.js",
  "bin": "HomeUploader.js",
  "scripts": {
    "test": "node HomeUploader.js",
    "archive": "tar --exclude='./content/**' --exclude='./node_modules/**' --exclude='./.git/**' --exclude='./.env' --exclude='./filing-saucer.tar.gz' -cvzf filing-saucer.tar.gz ./",
    "StartServer": "pm2 start HomeUploader.js",
    "StopServer": "pm2 stop HomeUploader.js",
    "RestartServer": "pm2 restart HomeUploader.js",
    "DeleteServer": "pm2 delete HomeUploader.js",
    "info": "pm2 describe HomeUploader.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SDPMrem71/HomeUploader"
  },
  "author": "Masoud Rahmani",
  "license": "GPL-3.0-only",
  "homepage": "https://github.com/SDPMrem71/HomeUploader",
  "dependencies": {
    "colors": "^1.4.0",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.1",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "multer": "^1.4.5-lts.1",
    "pm2": "^5.2.2",
    "prompt-sync": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
